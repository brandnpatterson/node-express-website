<div class="row justify-content-center">
  <div class="col-md-6">
    <h1>Users</h1>
    {{#each users}}
    <div class="card mb-3">
      <div class="card-body">
        <h3 class="mb-4">{{ firstname }} {{ lastname }}</h3 >
        <p>{{ email }}</p>
        <p>{{ username }}</p>
      </div>
    </div>
    {{/each}}
  </div>
</div>

{{#section 'footer'}}
  <script src="js/home.bundle.js"></script>
{{/section}}
